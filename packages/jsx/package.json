{
  "name": "@antv/infographic-jsx",
  "version": "0.0.1-beta.6",
  "description": "JSX runtime for AntV Infographic",
  "keywords": [
    "antv",
    "infographic",
    "jsx"
  ],
  "license": "MIT",
  "author": "Aarebecca",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    },
    "./jsx-runtime": {
      "types": "./esm/jsx-runtime.d.ts",
      "import": "./esm/jsx-runtime.js",
      "require": "./lib/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./esm/jsx-runtime.d.ts",
      "import": "./esm/jsx-runtime.js",
      "require": "./lib/jsx-runtime.js"
    }
  },
  "main": "./lib/index.js",
  "jsdelivr": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "lib",
    "esm",
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:esm build:cjs build:umd size",
    "build:cjs": "MODULE=cjs vite build",
    "build:esm": "MODULE=esm vite build && tsc --emitDeclarationOnly --outDir esm",
    "build:test": "tsc --noEmit -p __tests__",
    "build:umd": "ENTRY=index vite build --config vite.config.umd.ts && ENTRY=jsx-runtime vite build --config vite.config.umd.ts",
    "ci": "run-s test build lint",
    "clean": "rm -rf lib esm dist tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.tsx && npm run build:test",
    "size": "size-limit",
    "test": "vitest --run --coverage",
    "watch": "MODULE=esm vite build --watch"
  },
  "devDependencies": {
    "@types/react": "^19.1.12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "size-limit": [
    {
      "gzip": true,
      "limit": "500 Kb",
      "path": "dist/index.umd.js"
    },
    {
      "limit": "500 Kb",
      "path": "dist/index.umd.js"
    }
  ]
}
